<?xml version="1.0" encoding="ASCII"?>
<!DOCTYPE spec SYSTEM "dtd/attributespec.dtd">

<spec name="JVM Component Node" type="node">
	<attribute id="name"/>
	<attribute id="type"/>
	<attribute id="guid"/>
	
	<extension-attribute id="CORBA-enabled" action="yes:loadspec(CORBAJava,node);no:nop()"/>
	<extension-attribute id="launcher" action="yes:addchildren(launcher,1,attaching-connector,1);no:nop()"/>
	
	<child type="source-mapper" multiplicity="1"/>
</spec>